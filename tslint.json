{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-react"],
  "jsRules": {},
  "rules": {
    "interface-name": false,
    "member-access": false,
    "quotemark": false,
    "semicolon": [true, "always", "ignore-bound-class-methods"],
    "variable-name": [true, "check-format", "allow-leading-underscore"]
  },
  "rulesDirectory": []
}
